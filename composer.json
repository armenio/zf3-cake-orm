  "authors": [
    {
      "name": "Rafael ArmÃªnio",
      "email": "rafael.armenio@gmail.com"
    }
  ],
  "require": {
    "container-interop/container-interop": "^1.1",
    "php": "^5.6 || ^7.0",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-cache": "^2.7.1",
